@model IEnumerable<Student>
@{ 
    ViewBag.Title = "学生列表";
        }
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>姓名</th>
            <th>邮箱</th>
            <th>班级</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Emial</td>
                <td>@student.ClassName</td>
            </tr>
        }
    </tbody>
</table>